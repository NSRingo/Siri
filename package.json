{
	"name": "@iringo/siri",
	"organizationName": " iRingo",
	"displayName": " iRingo: ⭕ Siri",
	"description": "全面自定义「Siri」中的「Siri 请求」和「Siri 建议」功能。",
	"homepage": "https://NSRingo.github.io/guide/Siri",
	"openUrl": "http://boxjs.com/#/app/iRingo.Siri",
	"icon": "https://developer.apple.com/assets/elements/icons/siri/siri-128x128.png",
	"keywords": [],
	"contributors": [
		"VirgilClyne[https://github.com/VirgilClyne]"
	],
	"system": [
		"iOS",
		"iPadOS",
		"macOS"
	],
	"systemVersion": 15,
	"license": "Apache-2.0",
	"type": "module",
	"repository": {
		"type": "git",
		"url": "https://github.com/NSRingo/Siri.git"
	},
	"directories": {
		"example": "example"
	},
	"scripts": {
		"serve": "webpack serve",
		"build": "rspack build",
		"build:dev": "rspack build -c rspack.dev.config.js --mode=development",
		"build:args:siri": "arguments-builder build -c arguments-builder.Siri.config.ts",
		"build:args:search": "arguments-builder build -c arguments-builder.Search.config.ts",
		"build:args:spotlight": "arguments-builder build -c arguments-builder.Spotlight.config.ts",
		"dts": "arguments-builder dts"
	},
	"browserslist": [
		"iOS >= 15"
	],
	"devDependencies": {
		"@iringo/arguments-builder": "^1.8.5",
		"@protobuf-ts/plugin": "^2.9.4",
		"@rspack/cli": "^1.0.13",
		"@rspack/core": "^1.0.13",
		"node-polyfill-webpack-plugin": "^4.1.0"
	},
	"dependencies": {
		"@nsnanocat/util": "^1.7.6",
		"@protobuf-ts/runtime": "^2.9.3",
		"pako": "^2.1.0"
	}
}
